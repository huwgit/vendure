{
    "name": "huwgit-vendure",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev:server": "ts-node ./src/index.ts",
        "dev:worker": "ts-node ./src/index-worker.ts",
        "dev": "concurrently npm:dev:*",
        "build": "tsc",
        "start:server": "node ./dist/index.js",
        "start:worker": "node ./dist/index-worker.js",
        "start": "concurrently yarn:start:*",
        "compile": "ts-node ./src/custom-admin-ui/compile-admin-ui.ts",
        "migration:generate": "ts-node migration generate",
        "migration:run": "ts-node migration run",
        "migration:revert": "ts-node migration revert",
        "codegen": "graphql-codegen --config codegen.json",
        "format": "prettier --write --ignore-unknown"
    },
    "dependencies": {
        "@pinelab/vendure-plugin-invoices": "^1.2.0",
        "@vendure/admin-ui-plugin": "2.1.3",
        "@vendure/asset-server-plugin": "2.1.3",
        "@vendure/core": "2.1.3",
        "@vendure/email-plugin": "2.1.3",
        "@vendure/migrate-v2": "^0.4.0",
        "@vendure/payments-plugin": "2.1.3",
        "better-sqlite3": "9.1.1",
        "dotenv": "16.3.1",
        "stripe": "^13.7.0",
        "typescript": "5.1.6"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^5.0.0",
        "@graphql-codegen/typed-document-node": "^5.0.1",
        "@graphql-codegen/typescript": "^4.0.1",
        "@graphql-codegen/typescript-operations": "^4.0.1",
        "@vendure/ui-devkit": "2.1.3",
        "concurrently": "8.2.2",
        "prettier": "^3.1.0",
        "ts-node": "^10.9.1"
    }
}
